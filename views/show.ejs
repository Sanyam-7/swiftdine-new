<% include ./partials/header.ejs %>
<div class="flex flex-wrap gap-10 px-10 mt-10">
    <% for(let i =0; i<user.posts.length; i++) { %>

        <div class="cards flex flex-wrap gap-10 px-10 mt-10  ">
            <div class="card">
                <div class="w-52 h-40 bg-white rounded-lg overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= user?.posts[i]?.image  %>" alt="">
                </div>
              
            </div>
        </div>
    <% } %>
</div>

